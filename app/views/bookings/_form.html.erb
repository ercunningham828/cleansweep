
<%= form_for @booking do |f|%>
    <div class="form-group col-md-12">
        <%=f.label :property%>
        <%= f.collection_select(:property_id, @properties, :id, :name_and_address)%>
      </div>
       <div class="form-group col-md-12">
         <%=f.label :date %>
        <%= f.date_field :date, class: 'form-control'%>
      </div>

    <div class="col-md-12">
       <%= f.submit "Next", class: 'btn btn-success' %>
    </div>
  <%end%>

